{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error"],
        "import/extensions": 0,
        "no-use-before-define": 0
    }
}
